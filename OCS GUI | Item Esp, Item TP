--UI-- 
local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wall%20v3')))()

local w = library:CreateWindow("OCS") -- Creates the window

local b = w:CreateFolder("ESP Main") -- Creates the folder(U will put here your buttons,etc)
local c = w:CreateFolder("Rare Item ESP")
local d = w:CreateFolder("Item TPs")
b:Button("Item Esp",function()
    ItemEsp()
end)

c:Button("Launch Code ESP",function()
    ItemEspLC()
end)

b:Button("Clear Esp",function()
    ItemEspClear()
end)

d:Button("LaunchCodes TP",function()
    TeleportToLC()
end)

d:Button("FEV-Serum TP",function()
    FEVSerumTP()
end)

d:Button("RAD-Serum TP",function()
    RADSerumESP()
end)

c:Button("RAD-Serum ESP",function()
    RADSerumESP()
end)

c:Button("FEV-Serum ESP",function()
    FEVSerumESP()
end)

c:Button("APA ESP",function()
  APAESP()
end)

d:Button("APA TP",function()
  APATP()
end)



