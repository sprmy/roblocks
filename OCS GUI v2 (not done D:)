--FUNCTIONS--
local function GetItems()
    for i,v in next, nodepath:GetDescendants() do
        if v.Name == "Part" then
        end
    end
end
local function ItemEsp()
    local nodepath = game:GetService("Workspace").WorkspaceStuff.LootNodes
    for i,v in next, nodepath:GetDescendants() do
        if v.Name == "Part" then
            local bbG = Instance.new("BillboardGui")
            local TxtLbl = Instance.new("TextLabel")
    
            bbG.Parent = v
            bbG.AlwaysOnTop = true
            bbG.Size = UDim2.new(0,15,0,15)
            
            TxtLbl.Parent = bbG
            TxtLbl.BackgroundColor3 = Color3.fromRGB(0,255,0)
            TxtLbl.BackgroundTransparency = 1
            TxtLbl.Size = UDim2.new(5,0,5,0)
            TxtLbl.TextColor3 = Color3.fromRGB(0,255,0)
            TxtLbl.Text = v.Parent.Name
            TxtLbl.TextScaled = false
            game.Workspace.WorkspaceStuff.LootNodes.Lootnode:ChildAdded(ItemEsp)
        end
    end
end
local function ItemEspRare()
    local items = {
    "LaunchCodes",
    "Binoculars",
    "Minigun",
    "Laser Gatling",
    "RAD-Serum",
    "FEV-Serum",
    "T-60",
    "APA",
    "Tesla APA",
    "T-50",
    "T-51",
    "Note",
    "T-45",
}
    local nodepath = game:GetService("Workspace").WorkspaceStuff.LootNodes
    for i,v in next, nodepath:GetDescendants() do
        if v.Name == "Part" then
            if table.find(items,v.Parent.Name) then
                local bbG = Instance.new("BillboardGui")
                local TxtLbl = Instance.new("TextLabel")
    
            bbG.Parent = v
            bbG.AlwaysOnTop = true
            bbG.Size = UDim2.new(0,15,0,15)
            
            TxtLbl.Parent = bbG
            TxtLbl.BackgroundColor3 = Color3.fromRGB(0,255,0)
            TxtLbl.BackgroundTransparency = 1
            TxtLbl.Size = UDim2.new(5,0,5,0)
            TxtLbl.TextColor3 = Color3.fromRGB(0,0,255)
            TxtLbl.Text = v.Parent.Name
            TxtLbl.TextScaled = false
            game.Workspace.WorkspaceStuff.LootNodes.Lootnode:ChildAdded(ItemEsp)
            end
        end
    end
end
local function ItemEspClear()
    local nodepath = game:GetService("Workspace").WorkspaceStuff.LootNodes
    for i,v in next, nodepath:GetDescendants() do
        if v.Name == "Part" then
                v.BillboardGui:Destroy()
        end
    end
end
local function TeleportToLC()
    local items = {
    "LaunchCodes",
    "Binoculars",
    "Minigun",
    "Laser Gatling",
    "RAD-Serum",
    "FEV-Serum",
    "T-60",
    "APA",
    "Tesla APA",
    "T-50",
    "T-51",
    "Note",
    "T-45",
}
    local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
    local nodepath = game:GetService("Workspace").WorkspaceStuff.LootNodes
    for i,v in next, nodepath:GetDescendants() do
        if v.Name == "Part" then
            if table.find(items,v.Parent.Name) then
                plr.CFrame = v.CFrame
            end
        end
    end
end
local function RADSerumTP()
  local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
    local nodepath = game:GetService("Workspace").WorkspaceStuff.LootNodes
    for i,v in next, nodepath:GetDescendants() do
        if v.Name == "Part" then
            if v.Parent.Name == "RAD-Serum" then
                plr.CFrame = v.CFrame
            end
        end
    end
end
local function FEVSerumTP()
  local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
